
def build(c):
    tg = c( features = 'c cprogram',
            source = 'neart.l neart.y structs.c main.c visual/dot_syntax_tree.c',
            cflags = '-g',
            debug = True,
            target = 'neartc',
            use = 'NEART',
            includes = ['.', 'visual', '../third/klib'])
